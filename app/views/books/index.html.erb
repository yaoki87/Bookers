<h2>New book</h2>
<%= form_with model: @book, local:true do |f| %>

  <h4>title</h4>
  <%= f.text_field :title %>

  <h4>body</h4>
  <%= f.text_area :body %>

  <br>
  <%= f.submit 'Create Book' %>

<% end %>